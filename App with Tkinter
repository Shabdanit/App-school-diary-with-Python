from tkinter import*

#This functions is for button
def DateOfMonday():
    print("Math: " + input_text.get() + " " + "scr: " + score1_text.get())
    print("EngLang: " + input1_text.get() + " " + "scr: " + score2_text.get())
    print("RusLang: " + input2_text.get() + " " + "scr: " + score3_text.get())
    print("Chemistry: " + input3_text.get() + " " + "scr: " + score4_text.get())

def DateOfTuesday():
    print("Math: " + input5_text.get() + " " + "scr: " + score5_text.get())
    print("Physics: " + input6_text.get() + " " + "scr: " + score6_text.get())
    print("KyrgLang: " + input7_text.get() + " " + "scr: " + score7_text.get())
    print("History: " + input8_text.get() + " " + "scr: " + score8_text.get())

def DateOfWensday():
    print("СompScience: " + input9_text.get() + " " + "scr: " + score9_text.get())
    print("Physics: " + input10_text.get() + " " + "scr: " + score10_text.get())
    print("Geometry: " + input11_text.get() + " " + "scr: " + score11_text.get())
    print("CompGraphics: " + input12_text.get() + " " + "scr: " + score12_text.get())

def DateOfThursday():
    print("Drawing: " + input13_text.get() + " " + "scr: " + score13_text.get())
    print("Physics: " + input14_text.get() + " " + "scr: " + score14_text.get())
    print("EngLang: " + input15_text.get() + " " + "scr: " + score15_text.get())
    print("Chemistry: " + input16_text.get() + " " + "scr: " + score16_text.get())

def DateOfFriday():
    print("Drawing: " + input17_text.get() + " " + "scr: " + score17_text.get())
    print("Physics: " + input18_text.get() + " " + "scr: " + score18_text.get())
    print("EngLang: " + input19_text.get() + " " + "scr: " + score19_text.get())
    print("Physics: " + input20_text.get() + " " + "scr: " + score20_text.get())


lol = Tk()



lol.configure()

lol.title("Timetable")
#input text for timetable
input_text = StringVar()
input1_text = StringVar()
input2_text = StringVar()
input3_text = StringVar()
score1_text = StringVar()
score2_text = StringVar()
score3_text = StringVar()
score4_text = StringVar()
#Tuesday
input5_text = StringVar()
input6_text = StringVar()
input7_text = StringVar()
input8_text = StringVar()
score5_text = StringVar()
score6_text = StringVar()
score7_text = StringVar()
score8_text = StringVar()
#Wensday
input9_text = StringVar()
input10_text = StringVar()
input11_text = StringVar()
input12_text = StringVar()
score9_text = StringVar()
score10_text = StringVar()
score11_text = StringVar()
score12_text = StringVar()
#Thursday
input13_text = StringVar()
input14_text = StringVar()
input15_text = StringVar()
input16_text = StringVar()
score13_text = StringVar()
score14_text = StringVar()
score15_text = StringVar()
score16_text = StringVar()
#Thursday
input17_text = StringVar()
input18_text = StringVar()
input19_text = StringVar()
input20_text = StringVar()
