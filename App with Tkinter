score18_text = StringVar()
score19_text = StringVar()
score20_text = StringVar()

#label
subject = Label(lol, text='Subject',font = ('arial 14'), bd = 10).grid(row=0)

tasks = Label(lol, text='Task',font = ('arial 14'), bd = 10).grid(row=0, column = 1)

scores = Label(lol, text='score',font = ('arial 14'), bd = 10).grid(row=0, column = 2)

li1 = Label(lol, text='Math',font = ('arial'), bd = 10).grid(row=1)

li2 = Label(lol, text='EngLang',font = ('arial'), bd = 10).grid(row=2)

li3 = Label(lol, text='RusLang',font = ('arial'), bd = 10).grid(row=3)

li4 = Label(lol, text='Chemistry',font = ('arial'), bd = 10).grid(row=4)
#Entry
el = Entry(lol,textvariable = input_text).grid(row = 1, column = 1)

e2 = Entry(lol,textvariable = input1_text).grid(row = 2, column = 1)

e3 = Entry(lol,textvariable = input2_text).grid(row = 3, column = 1)

e4 = Entry(lol,textvariable = input3_text).grid(row = 4, column = 1)

#scores
score1 = Entry(lol,textvariable = score1_text,width = 5,justify = "left").grid(row = 1, column = 2)

score2 = Entry(lol,textvariable = score2_text,width = 5,justify = "left").grid(row = 2, column = 2)

score3 = Entry(lol,textvariable = score3_text,width = 5,justify = "left").grid(row = 3, column = 2)

score4 = Entry(lol,textvariable = score4_text,width = 5,justify = "left").grid(row = 4, column = 2)



#Button
